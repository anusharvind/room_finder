<p id="notice"><%= notice %></p>

<h1>Listing Room Facilities</h1>

<table>
  <thead>
    <tr>
      <th>Tv</th>
      <th>Wasing machine</th>
      <th>Ac</th>
      <th>Basic appliance</th>
      <th>Two wheeler parking</th>
      <th>Four wheeler parking</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_facilities.each do |room_facility| %>
      <tr>
        <td><%= room_facility.tv %></td>
        <td><%= room_facility.wasing_machine %></td>
        <td><%= room_facility.ac %></td>
        <td><%= room_facility.basic_appliance %></td>
        <td><%= room_facility.two_wheeler_parking %></td>
        <td><%= room_facility.four_wheeler_parking %></td>
        <td><%= link_to 'Show', room_facility %></td>
        <td><%= link_to 'Edit', edit_room_facility_path(room_facility) %></td>
        <td><%= link_to 'Destroy', room_facility, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room facility', new_room_facility_path %>
